  <story>
      <para style="IZQUIERDA">[[repeatIn(objects,'o')]]</para>
      <section>
        <para style="IZQUIERDA"></para>
        <blockTable colWidths="20mm,20mm,20mm,20mm,20mm,20mm,105mm,34mm" style="TABLA_BODY" repeatRows="1">
          <tr>
            <td> <para style="CENTRO7N">Origen</para></td>
            <td> <para style="CENTRO7N">Lote de produccion</para></td>
            <td> <para style="CENTRO7N">Fecha</para></td>
            <td> <para style="CENTRO7N">Factura</para></td>
            <td> <para style="CENTRO7N">Picking</para></td>
            <td> <para style="CENTRO7N">Producto</para></td>
            <td> <para style="CENTRO7N">Cliente</para></td>
            <td> <para style="CENTRO7N">Vendedor</para></td>
          </tr>
          <tr> [[repeatIn(o.line_ids,'l')]]
