            <td> <para style="CENTRO7">[[ l.prod_lot_id.name ]]</para></td>
            <td> <para style="CENTRO7">[[ formatLang(l.date_invoice, date='True') ]]</para></td>
            <td> <para style="CENTRO7">[[ l.invoice_id.number ]]</para></td>
            <td> <para style="CENTRO7">[[ l.picking_id and l.picking_id.name or '']]</para></td>
            <td> <para style="CENTRO7">[[ l.product_id.default_code ]]</para></td>
            <td> <para style="IZQUIERDA7">[[ l.partner_id.name ]]</para></td>
            <td> <para style="CENTRO7">[[ l.invoice_id.user_id.name ]]</para></td>
