'view/discount_partner.xml',
 -->

        <record model="ir.ui.view" id="view_discount_partner_search">
            <field name="name">discount.partner.search</field>
            <field name="model">discount.partner</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="">
                    <group>
                        <filter string="" icon="gtk-ok" domain="[]" context="{'group_by':'name'}"/>
                    </group>
                    <field name="name"/>
                    <field name="discount_percentage"/>
                </search>
            </field>
        </record>


        <record model="ir.ui.view" id="view_discount_partner_tree">
            <field name="name">discount.partner.tree</field>
