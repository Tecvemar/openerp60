            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name"/>
                    <field name="discount_percentage"/>
                </tree>
            </field>
        </record>


        <record model="ir.ui.view" id="view_discount_partner_form">
            <field name="name">discount.partner.form</field>
            <field name="model">discount.partner</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="">
                    <field name="name"/>
                    <field name="discount_percentage"/>
                </form>
            </field>
        </record>

    </data>
