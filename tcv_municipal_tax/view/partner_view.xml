<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_partner_tcv_municipal_tax_wh_form" model="ir.ui.view">
            <field name="name">res.partner.tcv.municipal.tax.wh.form</field>
            <field name="model">res.partner</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="stock.view_partner_property_form"/>
            <field name="arch" type="xml">
                <xpath expr='//field[@name="property_stock_supplier"]' position='after'>
                    <field name="property_stock_purchase"/>
                </xpath>
                <xpath expr='//field[@name="company_id"]' position='after'>
                    <field name="property_patent"/>
                </xpath>
            </field>
        </record>



    </data>
</openerp>

