<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="view_tcv_txt_check_export_vzla_form">
            <field name="name">tcv.txt.check.export.vzla.form</field>
            <field name="model">tcv.txt.check.export.vzla</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Export TXT Venezuela">
                    <separator string="TXT file" colspan="4"/>
                    <group col="8" colspan="8">
                        <field name="name" colspan="6"/>
                        <field name="csv_file" nolabel="1" readonly="1" fieldname="name" colspan="2"/>
                    </group>
                    <newline/>
                    <button name="button_create_csv" string="Create TXT file" icon="gtk-ok"
                            type="object" colspan="2"/>
                    <button icon='gtk-cancel' special="cancel" string="_Close"
                            colspan="2"/>
                </form>
            </field>
        </record>

    </data>
</openerp>

